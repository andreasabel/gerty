-- Examples for the builtin pairs

curry : (l : Level) -> (a : Type l) -> (b : Type l) -> (c : Type l) -> (a -> b -> c) -> (a * b) -> c
curry = \l -> \a -> \b -> \c -> \f -> \p -> let (x, y) = p in f x y


pairId : (l : Level) -> (a : Type l) -> (b : Type l) -> (a * b) -> (a * b)
pairId = \l -> \a -> \b -> \p -> let (x, y) = p in (x, y)
