t1 : (l1 l2 : Level) (a : Type l1) (b : Type l2) (e : a -> b) (t : a) -> Id l2 b ((\(x : a) -> e x) t) (e t)
t1 = \l1 l2 a b e t -> refl l2 b (e t)
