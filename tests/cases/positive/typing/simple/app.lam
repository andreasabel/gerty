-- testing applications

-- application of lambda
test1 : Level
test1 = (\(x : Level) -> x) lzero


test2 : Level -> Level
test2 = \x -> x


-- application of a lambda definition
test3 : Level
test3 = test2 lzero


-- alias for a builtin
test4 : (l : Level) -> Type (lsuc l)
test4 = Type


-- application of a definition to a definition
test5 : Type (lsuc test3)
test5 = test4 test3
