id : (a : [.0, .2] Type 0) -> (x : [.1, .0] a) -> a
id = \a -> \x -> x

pair : (a : [.0, .3] Type 0)
    -> (b : [.0, .2] Type 0)
    -> (f : [.1, .0] ((x : [.1, .0] a) -> (y : [.1, .0] a) -> b))
    -> (z : [.2 , .0] a) -> b
pair = \a -> \b -> \f -> \z -> f z z